<div position: absolute; left: 300px; >
  <form method='post' action='/sessions'>
    <input type='hidden' name='_method' value='delete'>
    <input type='submit' value='Sign out'>
  </form>
</div>

<h1> Your filtered links are: </h1>
<ul id='links'>
  <% @links.each do |link| %>
  <% if link.tags.map(&:tags).include? "#{@search}"  %>
    <li>
      URL:    <%= link.url %>
      TITLE:  <%= link.title %>
      TAG     <%= link.tags.map(&:tags) %>
    </li>
  <% end %>
  <% end %>
</ul>
